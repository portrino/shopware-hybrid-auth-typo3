<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="port1_hybrid_auth_typo3.configuration_service"
                 class="Port1HybridAuthTypo3\Service\ConfigurationService"
                 decorates="port1_hybrid_auth.configuration_service"
                 public="false"
        >
            <argument type="service" id="shopware_storefront.context_service" />
            <argument type="service" id="Config" />
            <argument type="service" id="snippets" />
            <argument type="service" id="port1_hybrid_auth_typo3.configuration_service.inner" />
        </service>

        <service id="port1_hybrid_auth.typo3_authentication_service" class="Port1HybridAuthTypo3\Service\AuthenticationService\Typo3AuthenticationService">
            <argument>Typo3</argument>
            <argument type="service" id="port1_hybrid_auth.configuration_service" />
        </service>
        <service id="port1_hybrid_auth_typo3.typo3_authentication_service" class="Port1HybridAuthTypo3\Service\AuthenticationService\Typo3AuthenticationService">
            <argument>Typo3</argument>
            <argument type="service" id="port1_hybrid_auth.configuration_service" />
        </service>
    </services>
</container>
{
    "name": "portrino/shopware-hybridauthtypo3",
    "description": "Shopware plugin for oauth2 login via TYPO3",
    "license": "proprietary",
    "type": "shopware-plugin",
    "authors": [
        {
            "name": "Andre Wuttig",
            "email": "wuttig@portrino.de",
            "homepage": "http://www.portrino.de",
            "role": "Developer"
        },
        {
            "name": "Axel Boeswetter",
            "email": "boeswetter@portrino.de",
            "homepage": "http://www.portrino.de",
            "role": "Developer"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "portrino/shopware-hybrid-auth": "~1.2.3"
    },
    "extra": {
        "installer-name": "Port1HybridAuthTypo3"
    },
    "scripts": {
        "post-install-cmd": [
            "Port1HybridAuthTypo3\\Composer\\AdditionalHybridAuthProvider::installTypo3"
        ],
        "post-update-cmd": [
            "Port1HybridAuthTypo3\\Composer\\AdditionalHybridAuthProvider::installTypo3"
        ]
    },
    "autoload": {
        "psr-4": {
            "Port1HybridAuthTypo3\\Composer\\": "Composer/"
        }
    }
}